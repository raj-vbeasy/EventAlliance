h1,h2,h3,h4,h5,h6 {
    font-family: 'Rubik', sans-serif !important;
}

.btn {
    font-family: 'Rubik', sans-serif !important;
}

.m-menu__link-text {
    font-weight: bold !important;
}

.m-hrader-search-form__help {
    color: #ffffff !important;
}

.m-hrader-search-form__select option {
    color: #575962;
}

.m-link {
    color: #02af9a !important;
}

.m-brand {
    background: #2f6f93 !important;
}

.profile_team_img {
    border-radius: 50%;
    height: 50px;
    width: 50px;
}

.member_img_vote_1{
	border-radius: 50%;
    height: 50px;
    width: 50px;
	border: solid 3px green;
}

.member_img_vote_0{
	 border-radius: 50%;
    height: 50px;
    width: 50px;
	border: solid 3px red;
}

.member_img_vote_null{
	border-radius: 50%;
    height: 50px;
    width: 50px;
	border: solid 3px grey;
}

.artist_availability_sidepanel {
    position: absolute;
    right: 25px;
    top: 25px;
}

.event-date{
    background-color:  #e5e5e5;
    padding: 5px 20px 5px 20px;
    text-align: center;
    line-height: 22px;
}

.m-subheader-search{
    padding-top: 100px !important;
    margin-bottom: 40px;
}

.event-approval-table-img{
    border-radius: 50%;
    height:35px; 
    width:35px;

}


.event-approval-td-div {
    text-align: center;
}
/*placeholder color*/
::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
    color: #a2b0bb !important;
    opacity: 1; /* Firefox */
}

:-ms-input-placeholder { /* Internet Explorer 10-11 */
    color: #a2b0bb !important;
}

::-ms-input-placeholder { /* Microsoft Edge */
    color: #a2b0bb !important;
}


/* artist details sub-heder menu*/

.m-tabs-line a.m-tabs__link,
.m-tabs-line.nav.nav-tabs .nav-link {
    background: 0 0;
    color: #ffffff !important;
}

.m-tabs-line.m-tabs-line--white a.m-tabs__link:hover,
.m-tabs-line.m-tabs-line--white.nav.nav-tabs .nav-link:hover {
    border-bottom: 1px solid #ffffff
}

.m-tabs-line.m-tabs-line--white a.m-tabs__link.active,
.m-tabs-line.m-tabs-line--white a.m-tabs__link:hover,
.m-tabs-line.m-tabs-line--white.nav.nav-tabs .nav-link.active,
.m-tabs-line.m-tabs-line--white.nav.nav-tabs .nav-link:hover {
    color: #ffffff;
    border-bottom: 1px solid #ffffff
}

.m-tabs-line.m-tabs-line--white a.m-tabs__link.active > i,
.m-tabs-line.m-tabs-line--white a.m-tabs__link:hover > i,
.m-tabs-line.m-tabs-line--white.nav.nav-tabs .nav-link.active > i,
.m-tabs-line.m-tabs-line--white.nav.nav-tabs .nav-link:hover > i {
    color: #ffffff
}

.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white a.m-tabs__link:hover,
.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white.nav.nav-tabs .nav-link:hover {
    border-bottom: 1px solid #ffffff
}

.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white a.m-tabs__link.active,
.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white a.m-tabs__link:hover,
.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white.nav.nav-tabs .nav-link.active,
.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white.nav.nav-tabs .nav-link:hover {
    color: #ffffff;
    border-bottom: 1px solid #ffffff
}

.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white a.m-tabs__link.active > i,
.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white a.m-tabs__link:hover > i,
.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white.nav.nav-tabs .nav-link.active > i,
.m-tabs-line.m-tabs-line--skin-dark.m-tabs-line--white.nav.nav-tabs .nav-link:hover > i {
    color: #ffffff
}

