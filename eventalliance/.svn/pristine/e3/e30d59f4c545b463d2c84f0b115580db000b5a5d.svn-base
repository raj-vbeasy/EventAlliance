/*header panel btn custom css*/
.btn-custom-header-panel{
    position: absolute; 
    top:-57px;
    color: #fff;
    background-color: #2a9c85;
    border-color: #299983;
    right:0;
    margin-right: 40px;
    padding: 15px 15px;
    -webkit-box-shadow: 0px 16px 26px -9px rgba(0,0,0,0.75) !important;
    -moz-box-shadow: 0px 16px 26px -9px rgba(0,0,0,0.75) !important;
    box-shadow: 0px 16px 26px -9px rgba(0,0,0,0.75) !important;
    max-width: 100% !important;
}

.header-floating-action-button{
    position: absolute;
    top: 114px;
    z-index: 5;
}

.header-floating-action-button-event{
    position: absolute;
    top: 228px;
    z-index: 5;
}

.header-floating-action-button-event-details{
        position: absolute;
    top: 216px;
    z-index: 5;
}